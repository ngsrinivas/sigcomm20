  </div>
  {% else -%}
  <div style="width: 80%">
  <p class="paper-header">
    <h2>{{ program_item.title }}</h2>
  </p>
  <p> {{ program_item.authors }} </p>
  </div>
  {% endif -%}
</li>
<!-- end single item in a session -->


{%  endif -%}
